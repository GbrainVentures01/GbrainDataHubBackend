{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "first_name": {
      "type": "string",
      "required": true,
      "configurable": false
    },
    "last_name": {
      "type": "string",
      "required": true,
      "configurable": false
    },
    "phone_number": {
      "type": "string",
      "required": true,
      "configurable": false
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "pin": {
      "type": "decimal",
      "maxLength": 4,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "airtime_orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::airtime-order.airtime-order",
      "mappedBy": "user"
    },
    "AccountNumber": {
      "type": "string",
      "unique": true
    },
    "AccountBalance": {
      "default": 0,
      "type": "decimal"
    },
    "BankName": {
      "type": "string"
    },
    "exam_pins_purchases": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::exam-pin-order.exam-pin-order",
      "mappedBy": "user"
    },
    "electricity_bills": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::electricity-order.electricity-order",
      "mappedBy": "user"
    },
    "data_gifting_orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::data-gifting-order.data-gifting-order",
      "mappedBy": "user"
    },
    "sme_data_orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::sme-data-order.sme-data-order",
      "mappedBy": "user"
    },
    "tv_and_cables_orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tvcables-order.tvcables-order",
      "mappedBy": "user"
    },
    "sell_airtimes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::sell-airtime.sell-airtime",
      "mappedBy": "user"
    },
    "account_fundings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::account-funding.account-funding",
      "mappedBy": "user"
    }
  }
}
