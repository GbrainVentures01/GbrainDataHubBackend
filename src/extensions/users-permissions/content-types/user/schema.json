{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "USERS DETAILS"
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "first_name": {
      "type": "string",
      "required": false,
      "configurable": true
    },
    "last_name": {
      "type": "string",
      "required": false,
      "configurable": true
    },
    "phone_number": {
      "type": "string",
      "required": true,
      "configurable": false
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "pin": {
      "type": "password",
      "maxLength": 4,
      "configurable": false,
      "private": true
    },
    "transactionPin": {
      "type": "password",

      "configurable": false,
      "private": true
    },
    "hasTransactionPin": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "biometricEnabled": {
      "type": "boolean",
      "default": false,
      "required": false
    },
    "biometricEnabledAt": {
      "type": "datetime",
      "required": false
    },
    "biometricDisabledAt": {
      "type": "datetime",
      "required": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "avatar": {
      "type": "string",
      "required": false,
      "configurable": true
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "airtime_orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::airtime-order.airtime-order",
      "mappedBy": "user"
    },
    "AccountBalance": {
      "default": 0,
      "type": "decimal"
    },
    "exam_pins_purchases": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::exam-pin-order.exam-pin-order",
      "mappedBy": "user"
    },
    "electricity_bills": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::electricity-order.electricity-order",
      "mappedBy": "user"
    },
    "data_gifting_orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::data-gifting-order.data-gifting-order",
      "mappedBy": "user"
    },
    "sme_data_orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::sme-data-order.sme-data-order",
      "mappedBy": "user"
    },
    "tv_and_cables_orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tvcables-order.tvcables-order",
      "mappedBy": "user"
    },
    "sell_airtimes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::sell-airtime.sell-airtime",
      "mappedBy": "user"
    },
    "account_fundings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::account-funding.account-funding",
      "mappedBy": "user"
    },
    "cg_data_orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::cg-data-order.cg-data-order",
      "mappedBy": "user"
    },
    "monnify_bank_details": {
      "displayName": "Monnify Accounts",
      "type": "component",
      "repeatable": true,
      "component": "monnify-accounts.monnify-accounts"
    },
    "hasAccountNum": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "updateBvn": {
      "type": "boolean",
      "default": false
    },
    "mtn_sme_1_data_orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::mtn-sme-1-data-order.mtn-sme-1-data-order",
      "mappedBy": "user"
    },
    "mtn_sme_2_data_orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::mtn-sme-2-data-order.mtn-sme-2-data-order",
      "mappedBy": "user"
    },
    "mtn_coupon_data_orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::mtn-coupon-data-order.mtn-coupon-data-order",
      "mappedBy": "user"
    },
    "payvessel_accounts": {
      "displayName": "payvessel_accounts",
      "type": "component",
      "repeatable": true,
      "component": "payvessel-accounts.payvessel-accounts"
    },
    "cryptoWalletBalance": {
      "type": "float",
      "default": 0
    },
    "giftCardBalance": {
      "type": "float",
      "default": 0
    },
    "verificationCodes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::verification-code.verification-code",
      "mappedBy": "user"
    }
  }
}
