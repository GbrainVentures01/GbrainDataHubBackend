{
  "kind": "collectionType",
  "collectionName": "data_gifting_orders",
  "info": {
    "singularName": "data-gifting-order",
    "pluralName": "data-gifting-orders",
    "displayName": "ALL GIFTING DATA ORDER",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "beneficiary": {
      "type": "string",
      "required": true
    },
    "amount": {
      "type": "decimal",
      "required": true
    },
    "plan": {
      "type": "string",
      "required": true
    },
    "network": {
      "type": "string",
      "required": true
    },
    "previous_balance": {
      "type": "decimal"
    },
    "current_balance": {
      "type": "decimal"
    },
    "status": {
      "type": "enumeration",
      "enum": ["pending", "delivered", "failed"],
      "default": "pending"
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "data_gifting_orders"
    },
    "TRX_Name": {
      "type": "string",
      "default": "Data (Gifting)"
    },
    "request_id": {
      "type": "string",
      "required": true
    }
  }
}
