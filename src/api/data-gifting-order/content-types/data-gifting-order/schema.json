{
  "kind": "collectionType",
  "collectionName": "data_gifting_orders",
  "info": {
    "singularName": "data-gifting-order",
    "pluralName": "data-gifting-orders",
    "displayName": "Data Gifting Order",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "beneficiary": {
      "type": "string",
      "required": true
    },
    "amount": {
      "type": "decimal",
      "required": true
    },
    "plan": {
      "type": "string",
      "required": true
    },
    "network": {
      "type": "string",
      "required": true
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "Pending",
        "Successful",
        "Failed"
      ],
      "default": "Pending"
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "data_gifting_orders"
    }
  }
}
