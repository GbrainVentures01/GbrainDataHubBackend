{
  "kind": "collectionType",
  "collectionName": "sell_airtimes",
  "info": {
    "singularName": "sell-airtime",
    "pluralName": "sell-airtimes",
    "displayName": "Sell Airtime Order",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "network": {
      "type": "string",
      "required": true
    },
    "amount": {
      "type": "decimal",
      "required": true
    },
    "account_number": {
      "type": "string",
      "required": false
    },
    "account_name": {
      "type": "string",
      "required": false
    },
    "bank_name": {
      "type": "string",
      "required": false
    },
    "phone_number": {
      "type": "string",
      "required": true
    },
    "request_id": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "sell_airtimes"
    },
    "TRX_Name": {
      "type": "string",
      "default": "Sell Airtime"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "Pending",
        "Sucessful",
        "Failed"
      ],
      "default": "Pending",
      "required": true
    },
    "screenshot": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "previous_balance": {
      "type": "decimal"
    },
    "current_balance": {
      "type": "decimal"
    }
  }
}
