{
  "kind": "collectionType",
  "collectionName": "electricity-orders",
  "info": {
    "singularName": "electricity-order",
    "pluralName": "electricity-orders",
    "displayName": "Electricity Bills ",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "request_id": {
      "type": "string",
      "required": true
    },
    "serviceID": {
      "type": "string",
      "required": true
    },
    "billersCode": {
      "type": "string",
      "required": true
    },
    "amount": {
      "type": "string",
      "required": true
    },
    "previous_balance": {
      "type": "decimal"
    },
    "current_balance": {
      "type": "decimal"
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "variation_code": {
      "type": "string",
      "required": true
    },
    "status": {
      "type": "enumeration",
      "enum": ["Pending", "Successful", "Failed"],
      "default": "Pending"
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "electricity_bills"
    },
    "TRX_Name": {
      "type": "string",
      "default": "Electricity"
    },
    "purchased_token": {
      "type": "string"
    }
  }
}
