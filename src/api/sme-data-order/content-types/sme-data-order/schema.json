{
  "kind": "collectionType",
  "collectionName": "sme_data_orders",
  "info": {
    "singularName": "sme-data-order",
    "pluralName": "sme-data-orders",
    "displayName": "SME Data Order",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "network": {
      "type": "string",
      "required": true
    },
    "ref": {
      "type": "string",
      "required": true
    },
    "amount": {
      "type": "integer",
      "required": true
    },
    "plan": {
      "type": "string",
      "required": true
    },
    "beneficiary": {
      "type": "string",
      "required": true
    },
    "previous_balance": {
      "type": "decimal"
    },
    "current_balance": {
      "type": "decimal"
    },
    "TRX_Name": {
      "type": "string",
      "default": "Data (SME)"
    },
    "status": {
      "type": "enumeration",
      "enum": ["pending", "delivered", "failed", "processing"],
      "default": "pending"
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "sme_data_orders"
    }
  }
}
